[user]
	name = Àlex Marlés
[color]
	status = auto
	branch = auto
	diff = auto
	interactive = auto
[core]
	editor = vim
[alias]
	gl = log --graph --pretty=format:'%C(yellow bold)%h%C(red bold)%d%Creset %s %C(green bold) %cn %C(cyan bold)(%cr)' --all
	gl2 = log --graph --oneline --decorate --all
	gs = status -sb
	ga = add .
	gc = commit -a
